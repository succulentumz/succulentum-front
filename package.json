{
  "name": "succulentumz-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.2.2",
  "scripts": {
    "start": "env-cmd --file .env.local vite",
    "build": "tsc && vite build",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,css,html,json,ts,tsx}\"",
    "types-check": "tsc --noEmit",
    "format:check": "prettier --check \"**/*.{js,jsx,css,json,ts,tsx}\"",
    "full-check": "npm types-check && npm format:check && npm lint"
  },
  "dependencies": {
    "@tanstack/react-query": "5.29.2",
    "@true-engineering/true-react-common-api-client": "0.4.0",
    "@true-engineering/true-react-platform-helpers": "0.4.5",
    "clsx": "2.1.0",
    "date-fns": "2.30.0",
    "i18next": "23.11.1",
    "i18next-browser-languagedetector": "7.2.1",
    "jss": "10.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.1.0",
    "react-jss": "10.10.0",
    "react-overlays": "5.2.1",
    "react-router-dom": "6.22.3",
    "react-transition-group": "4.4.5",
    "use-zustand": "0.2.0",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.1",
    "@types/node": "22.13.4",
    "@types/react": "18.2.77",
    "@types/react-datepicker": "6.0.3",
    "@types/react-dom": "18.2.25",
    "@types/react-transition-group": "4.4.10",
    "@vitejs/plugin-react": "4.2.1",
    "axios": "1.2.3",
    "env-cmd": "10.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-storybook": "0.8.0",
    "jsdom": "24.0.0",
    "prettier": "3.2.5",
    "type-fest": "4.33.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.6.0",
    "vite": "5.2.8",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.5.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
